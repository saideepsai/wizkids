<script>
  // Function to toggle profile details
  function toggleProfileDetails() {
    $(".profile-details").toggle();
  }

  // Click event for the profile link
  $("#profile-link").click(function (e) {
    e.preventDefault();
    toggleProfileDetails();
  });

  // Hover event for the main menu
  $(".main-menu").hover(
    function () {
      $(".right-column").css("transition", "margin-left 0.2s");
      $(".right-column").css("margin-left", "250px");
    },
    function () {
      $(".right-column").css("margin-left", "90px");
    }
  );

  // Function to handle logout
  function logout() {
    // Send a POST request to the logout route
    fetch('/logout', {
      method: 'POST',
    })
      .then(response => {
        if (response.ok) {
          // Redirect to the Wizkids page after successful logout
          window.location.href = '/';
        } else {
          console.error('Logout failed');
        }
      })
      .catch(error => {
        console.error('Logout failed:', error);
      });
  }
</script>


    
    
    
